import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		
		System.out.println("Digite um numero inteiro: ");
		int i = sc.nextInt();
		System.out.println("-----------");
		
		for(int n = 1; n <= 10; n++){
		    System.out.println(n * i);
		}
		
		
	}
}
